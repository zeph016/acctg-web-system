@inherits ListRFPVoucherCardBase

<MudTable Items="@VoucherList" Bordered ReadOnly='false'
            Dense Elevation='0' FixedHeader FixedFooter RowsPerPage="100">
    <ColGroup>
        <col />
        <col />
        <col />q
    </ColGroup>
    <HeaderContent>
        <MudTh Class='customheader-6 text-center'>
            Date
        </MudTh>
        <MudTh Class='customheader-6 text-center'>
            Control Number
        </MudTh>
        <MudTh Class='customheader-6 text-center'>
            Status
        </MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd Class='text-center'>
            @context.VoucherDate.ToString("MM/dd/yyyy")
        </MudTd>
        <MudTd Class='text-center'>
            @context.ControlNumber
        </MudTd>
        <MudTd Class='text-center'>
            @context.StatusName
        </MudTd>
    </RowTemplate>
</MudTable>


